{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime.version": "Lua 5.1",
  "runtime.builtin": {
    "basic": true,
    "coroutine": true,
    "math": true,
    "string": true,
    "table": true
  },
  "workspace.checkThirdParty": false,
  "workspace.ignoreDir": [
    "Libs",
    "Libs/**",
    "**/Libs",
    "**/Libs/**"
  ],
  "type.weakUnionCheck": true,
  "diagnostics.neededFileStatus": "Opened",
  "diagnostics.disable": [
    "invisible"
  ]
}
